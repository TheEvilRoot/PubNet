{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Worker": {
    "Interval": "00:15:00"
  },
  "PackageStorage": {
    "Path": "./packages",
    "PendingMaxAge": "00:05:00"
  },
  "Jwt": {
    "LifetimeInSeconds": 1209600
  }
}
